# Device Specific Board Configuration
CONFIG_BOARD_MUNCH=y

# Touchscreen
CONFIG_TOUCHSCREEN_FOCALTECH_3658U=y
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=y
CONFIG_TOUCHSCREEN_SUPPORT_NEW_GAME_MODE=y

# Enable QCOM RPMH Regulator
CONFIG_REGULATOR_QCOM_RPMH=y

# Enable PM8008 Regulator
CONFIG_REGULATOR_PM8008=y

# Enable MI US Proximity sensor
CONFIG_US_PROXIMITY=y

# Power supply drivers
# CONFIG_QPNP_FG_GEN4 is not set
# CONFIG_QPNP_SMB5 is not set
CONFIG_QPNP_SMB5_MUNCH=y
CONFIG_CHARGER_LN8000_MUNCH=y
CONFIG_FUEL_GAUGE_BQ27Z561_MUNCH=y
CONFIG_BQ2597X_CHARGE_PUMP_STANDALONE_MUNCH=y

# Three antenna WiFi
CONFIG_WIFI_THREE_ANTENNA=y

# USB PD
# CONFIG_USB_PD_POLICY is not set
CONFIG_USB_PD_POLICY_MUNCH=y

# Retry i2c operations on failure
CONFIG_I2C_RETRY=y

# Enable thermal dimming
CONFIG_THERMAL_DIMMING=y

# Enable Awinic AW22xxx leds
CONFIG_LEDS_AW22XXX=y

# Enable UFS 3.1 features
CONFIG_UFS3V1=y

# Enable Awinic haptics driver
# CONFIG_INPUT_AW8697_HAPTIC is not set
CONFIG_INPUT_AW8697_HAPTIC_MUNCH=y

# {
#   Enable fingerprint sensors
#   POCO F4: GOODIX
#   K40S: FPC_TEE
# }
CONFIG_FINGERPRINT_GOODIX=y
CONFIG_FINGERPRINT_FPC_TEE=y
